{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "trailing-comma": [ false ],
    "object-literal-sort-keys": false,
    "max-line-length": false,
    "interface-name": false,
    "no-console": false,
    "no-empty-interface": false,
    "quotemark": {
      "options": "single"
    },
    "variable-name": false
  },
  "rulesDirectory": []
}
